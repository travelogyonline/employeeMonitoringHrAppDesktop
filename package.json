{
  "name": "employeemonitoringhrappdesktop",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "main": "dist-electron/main.js",
  "scripts": {
    "dev:react": "vite",
    "dev:electron": "electron .",
    "lint": "eslint .",
    "preview": "vite preview",
    "build:react": "vite build",
    "build:electron": "vite build --mode production",
    "build": "npm run build:react && npm run build:electron",
    "start:electron": "electron dist-electron/main.js",
    "dist:mac": "npm run build && electron-builder --mac --arm64",
    "dist:linux": "npm run build && electron-builder --linux --x64",
    "dist:win": "npm run build && electron-builder --win --x64"
  },
  "dependencies": {
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "vite-plugin-electron": "^0.29.0"
  },
  "devDependencies": {
    "electron": "^39.1.2",
    "electron-builder": "^26.0.12",
    "@eslint/js": "^9.39.1",
    "@types/react": "^19.2.2",
    "@types/react-dom": "^19.2.2",
    "@vitejs/plugin-react": "^5.1.0",
    "babel-plugin-react-compiler": "^19.1.0-rc.3",
    "eslint": "^9.39.1",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.24",
    "globals": "^16.5.0",
    "vite": "^7.2.2"
  }
}